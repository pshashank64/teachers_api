        <div>
            <h1>To create batch</h1>
            <form action="/api/v1/create/create-batch" method="post">
                <input type="number" name="courseId" placeholder="course ID" />
                <input type="text" name="name" placeholder="name of course" />
                <input type="date" name="date" placeholder="date of batch scheduled" />
                <input type="time" name="time" placeholder="time to begin class" />
                <button type="submit" class="btn">Submit</button>
            </form>
            <h1 style="margin: 50px 0px 30px 0px;">To reschedule any batch</h1>
            <form action="/api/v1/reschedule/reschedule-batch" method="post">
                <input type="number" name="courseId" placeholder="course ID" />
                <input type="date" name="date" placeholder="new date" />
                <input type="time" name="time" placeholder="new time" />
                <button type="submit" class="btn">Submit</button>
            </form>
        </div>
        <script src="" async defer></script>
    </body>
